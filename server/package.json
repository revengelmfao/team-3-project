{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run build && node dist/server.js --ignore client",
    "dev": "npx nodemon",
    "build": "tsc",
    "seed": "node dist/seeds/index.js"
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": "ts,json,js",
    "exec": "npx tsc && node dist/server.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "eslint": "^9.19.0",
    "nodemon": "^3.1.3"
  }
}
